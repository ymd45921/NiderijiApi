    "build:d": "rimraf ./dist && tsc --outdir dist --emitDeclarationOnly",
    "release": "yarn build:pub && yarn publish",
    "pub": "yarn release"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "dotenv": "^16.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  }
