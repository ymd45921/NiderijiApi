{
  "name": "nideriji-api",
  "version": "0.2.4",
  "description": "API of Nideriji and Ohshenghuo.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Nanami",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.26.1"
  },
  "scripts": {
    "build": "rimraf ./build && tsc -d --outDir build",
    "clear": "rimraf ./build && rimraf ./dist",
    "build:to": "tsc -d --outDir",
